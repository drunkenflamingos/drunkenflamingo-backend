{
  "name": "josegonzalez/app",
  "description": "CakePHP skeleton app",
  "homepage": "https://github.com/josegonzalez/app",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "ext-apcu": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-pdo_sqlite": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "ext-redis": "*",
    "cakephp/cakephp": "3.3.*",
    "mobiledetect/mobiledetectlib": "2.*",
    "cakephp/migrations": "~1.0",
    "cakephp/plugin-installer": "*",
    "josegonzalez/dotenv": "2.*",
    "friendsofcake/crud": "4.*",
    "friendsofcake/crud-view": "0.*",
    "friendsofcake/bootstrap-ui": "^0.6.2",
    "friendsofcake/search": "1.*",
    "markstory/asset_compress": "3.*",
    "josegonzalez/cakephp-upload": "3.*",
    "josegonzalez/cakephp-queuesadilla": "0.3.*",
    "josegonzalez/queuesadilla": "*",
    "josegonzalez/php-error-handlers": "0.*",
    "friendsofcake/crud-users": "^0.3.2",
    "muffin/footprint": "dev-master",
    "josegonzalez/cakephp-version": "dev-master",
    "wyrihaximus/fly-pie": "^1.1",
    "ad7six/shadow-translate": "^0.5.3",
    "muffin/trash": "^1.1",
    "admad/cakephp-glide": "^2.0",
    "guzzlehttp/guzzle": "~6.0",
    "friendsofcake/fixturize": "^1.0",
    "muffin/slug": "^1.1",
    "bakkerij/notifier": "^1.2"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "cakephp/debug_kit": "~3.2",
    "phpunit/phpunit": "^6.0",
    "cakephp/bake": "^1.2",
    "cakephp/cakephp-codesniffer": "^2.4",
    "phploc/phploc": "^3.0"
  },
  "suggest": {
    "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
    "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
    "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
  },
  "autoload": {
    "psr-4": {
      "App\\": "src",
      "Admin\\": "./plugins/Admin/src",
      "Api\\": "./plugins/Api/src",
      "User\\": "./plugins/User/src",
      "CustomBootstrap\\": "./plugins/CustomBootstrap/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
      "Admin\\Test\\": "./plugins/Admin/tests",
      "Api\\Test\\": "./plugins/Api/tests",
      "User\\Test\\": "./plugins/User/tests",
      "CustomBootstrap\\Test\\": "./plugins/CustomBootstrap/tests"
    }
  },
  "scripts": {
    "compile": [
      "bin/cake migrations migrate",
      "bin/cake migrations migrate -p Josegonzalez/CakeQueuesadilla",
      "bin/cake migrations migrate -p Bakkerij/Notifier",
      "bin/cake asset_compress clear",
      "bin/cake asset_compress build"
    ],
    "post-install-cmd": "App\\Console\\Installer::postInstall",
    "post-create-project-cmd": "App\\Console\\Installer::postInstall",
    "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump",
    "check": [
      "@test",
      "@cs-check"
    ],
    "cs-check": "vendor/bin/phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests",
    "cs-fix": "vendor/bin/phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests",
    "test": "vendor/bin/phpunit --colors=always"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
